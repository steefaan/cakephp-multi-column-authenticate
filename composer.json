{
    "name": "steefaan/cakephp-multi-column-authenticate",
    "type": "cakephp-plugin",
    "description": "Provides multi column form authentications.",
    "keywords": [
        "cakephp",
        "authenticate"
    ],
    "homepage": "http://github.com/steefaan/cakephp-multi-column-authenticate",
    "authors": [
        {
            "name": "Stefan Dickmann",
            "role": "Author"
        }
    ],
    "license": "MIT",
    "support": {
        "source": "https://github.com/steefaan/cakephp-multi-column-authenticate",
        "issues": "https://github.com/steefaan/cakephp-multi-column-authenticate/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "MultiColumnAuthenticate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MultiColumnAuthenticate\\Test\\": "tests"
        }
    }
}
